<h1> Participants </h1>
<table>
  <tr>
    <th>||  Email  ||</th>
    <th>||  Resume  ||</th>
  </tr>
 
  <% @participants.each do |participant| %>
    <tr>
      <td>|| <%= participant.email %>           || </td>
      <td>|| <%= participant.resume %>        || </td>
      <td><%= link_to 'Show', participant_path(participant) %></td>
    </tr>
  <% end %>
</table>